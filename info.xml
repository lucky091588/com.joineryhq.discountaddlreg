  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.discountaddlreg</url>
    <url desc="Documentation">https://twomice.github.io/com.joineryhq.discountaddlreg/</url>
    <url desc="Support">https://twomice.github.io/com.joineryhq.discountaddlreg/#support</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2020-05-11</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed by Joinery for FPPTA.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Discountaddlreg</namespace>
  </civix>
</extension>
